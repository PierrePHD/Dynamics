AAJZ
PR_CritereNormeModes
DEBPROC PR_CritereNormeModes ;

    lg = DIMENSION g1.u;
    g1_fin = EXTR g1.u lg;
    g2_fin = EXTR g2.u lg;
    Val = g1_fin / g2_fin ;
    *Val_sign = Sign Val;

    f1Kf1 = XTMX f1 K;
    f12 = f1 - (f2*Val) ;
    f12Kf12 = XTMX f12 K;

    Res1 = f12Kf12/f1Kf1;

    g12 =  g1.u - (g2.u*Val);
    g1_g1   = PR_IntegrProdLine g1.u g1.u  dt ;
    g12_g12 = PR_IntegrProdLine g12 g12    dt ;

    Res2 = g12_g12/g1_g1;

    RESPRO Res1;
    RESPRO Res2;

FINPROC;
